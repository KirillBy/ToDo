}\n\n  deleteItem = (id) => {\n    this.setState(({todoData}) => {\n      const idx = todoData.findIndex((el) => el.id === id);\n      const before = todoData.slice(0, idx);\n      const after = todoData.slice(idx +1);\n      const newArray= [...before, ...after]\n      return {\n        todoData: newArray\n      }\n    })\n  };\n\n  addItem = (text, show) => {\n    this.setState(({todoData}) => {\n      const newArray = [...todoData, this.createTodoItem(text)];\n      return {\n        todoData: newArray,\n        show: false\n      }\n    })\n  }\n\n  toggleProperty = (arr, id, propName) => {\n    const idx = arr.findIndex((el) => el.id === id);\n    const oldItem = arr[idx];\n    const newItem = {...oldItem, [propName]: !oldItem[propName]};\n    return [...arr.slice(0, idx), newItem, ...arr.slice(idx +1)];\n    \n  }\n\n  onToggleImportant = (id) => {\n    this.setState(({todoData}) => {\n      return{\n        todoData: this.toggleProperty(todoData, id, 'important')\n      };\n    });\n  };\n\n  onToggleDone = (id) => {\n    this.setState(({todoData}) => {\n      return{\n        todoData: this.toggleProperty(todoData, id, 'done')\n      };\n    })\n  };\n\n  filterItems = (arr, term) => {\n    if (term.length === 0){\n      return arr;\n    }\n    return arr.filter((elem) => {\n      return elem.label.toLowerCase().indexOf(term.toLowerCase()) > -1;\n    })\n  }\n\n  changeTerm = (term) => {\n    this.setState({term});\n  }\n\n  onFilterChange = (filter) => {\n    this.setState({filter});\n  }\n\n  render() {\n    const { todoData, term, filter } = this.state;\n    const doneCount = todoData\n                      .filter((el) => el.done).length;\n    const todoCount = todoData.length - doneCount;\n    const visibleData = this.filter(this.filterItems(todoData, term), filter);\n    return (\n      <div className=\"todo-app\">\n        <AppHeader toDo={todoCount} done={doneCount} />\n        <div className=\"top-panel\">\n          <SearchPanel onSearch={this.changeTerm}/>\n          <ItemStatusFilter filter={filter} onFilterChange={this.onFilterChange}/>\n        </div>\n        <TodoList \n        todos={visibleData} \n        onDeleted={ this.deleteItem} \n        onToggleImportant={this.onToggleImportant}\n        onToggleDone = {this.onToggleDone}\n        />\n        <button\n          class=\"toggle-button\"\n          id=\"centered-toggle-button\"\n          hidden = {this.state.show}\n          onClick={e => {\n            this.showModal(e);\n          }}\n        > + </button>\n        <ItemAddForm \n        onAdd ={this.addItem}\n        onClose={this.showModal}\n        show={this.state.show}/>\n      </div>\n    );\n  };\n};\n\n","/home/kirill/ToDo/todo/src/components/item-status-filter/index.js",[],"/home/kirill/ToDo/todo/src/components/search-panel/index.js",[],"/home/kirill/ToDo/todo/src/components/app-header/index.js",[],"/home/kirill/ToDo/todo/src/components/item-add-form/index.js",[],"/home/kirill/ToDo/todo/src/components/todo-list/index.js",[],"/home/kirill/ToDo/todo/src/components/modal/index.js",[],"/home/kirill/ToDo/todo/src/components/item-status-filter/item-status-filter.js",[],"/home/kirill/ToDo/todo/src/components/search-panel/search-panel.js",[],"/home/kirill/ToDo/todo/src/components/app-header/app-header.js",[],"/home/kirill/ToDo/todo/src/components/item-add-form/item-add-form.js",[],"/home/kirill/ToDo/todo/src/components/todo-list/todo-list.js",[],"/home/kirill/ToDo/todo/src/components/modal/modal.js",[],"/home/kirill/ToDo/todo/src/components/todo-list-item/index.js",[],"/home/kirill/ToDo/todo/src/components/todo-list-item/todo-list-item.js",["76"],"import React, {Component} from 'react';\nimport './todo-list-item.css';\n\nconst TodoListItem = ({label, onDeleted, onToggleImportant, onToggleDone, done, important}) => {\n\n    let classNames = 'todo-list-item';\n    if(done) {\n      classNames += ' done';\n    }\n\n    if (important) {\n      classNames += ' important';\n    }\n  \n    return (\n      <span className={classNames}>\n        <span\n          className=\"todo-list-item-label\"\n          onClick={ onToggleDone}>\n          {label}\n        </span>\n  \n        <div className=\"btn btn-group\">\n          <button type=\"button\"\n                  className=\"btn btn-outline-primary\"\n                  onClick = {onToggleImportant}>\n                    Important\n          </button>\n    \n          <button type=\"button\"\n                  className=\" btn btn-outline-danger\"\n                  onClick={onDeleted}>\n                    Delete\n          </button>\n        </div>\n\n      </span>\n    );\n}\n\nexport default TodoListItem;",{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":8,"column":8,"nodeType":"83","messageId":"84","endLine":8,"endColumn":13},{"ruleId":"81","severity":1,"message":"85","line":1,"column":16,"nodeType":"83","messageId":"84","endLine":1,"endColumn":25},"no-native-reassign",["86"],"no-negated-in-lhs",["87"],"no-unused-vars","'Modal' is defined but never used.","Identifier","unusedVar","'Component' is defined but never used.","no-global-assign","no-unsafe-negation"]